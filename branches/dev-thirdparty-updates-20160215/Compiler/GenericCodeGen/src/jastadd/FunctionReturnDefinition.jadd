import java.util.ArrayList;

/**
 * A class representing the return values of a function.
 */
public class FunctionReturnDefinition {
	private ArrayList<FFunctionVariable> outputs;
	private FFunctionDecl decl;
	
	public FunctionReturnDefinition(FFunctionDecl decl) {
	    this.decl = decl;
		outputs = decl.myOutputs();
	}
	
	/*
	 * Additional methods added by respective *CodeGen.jrag files.
	 */
}