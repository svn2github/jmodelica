    eq FEquation.isPrePropagationEquation() {
        FAbstractVariable left = getLeft().isAccess() ? getLeft().asFIdUse().myFV() : null;
        FAbstractVariable right = getRight().isAccess() ? getRight().asFIdUse().myFV() : null;
        return left == right ? left : null;
    }
    
