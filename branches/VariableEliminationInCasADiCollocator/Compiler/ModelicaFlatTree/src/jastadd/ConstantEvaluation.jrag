  @Override
  public int FFunctionCallStmt.evaluate(AlgorithmEvaluator evaluator) {
	  CValue[] vals = getCall().evaluate(evaluator);
	  for (int i = 0, n = getNumLeft(); i < n; i++)
		  getLeft(i).assignToInFuncEval(evaluator, vals[i]);
	  return EVAL_CONT;
  }

