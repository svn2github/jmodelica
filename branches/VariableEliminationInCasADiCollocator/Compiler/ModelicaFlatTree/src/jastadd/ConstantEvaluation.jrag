    public void FRecordConstructor.mapComponents(Map<FIdUseExp, Set<FIdUseExp>> res, CValue val) {
        CValueRecord rec = val.record();
        for (int i = 0; i < getNumArg(); i++) {
            getArg(i).mapComponents(res, rec.getMember(i));
