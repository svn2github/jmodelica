    public void FIdUseExp.mapComponents(Map<FIdUseExp, Set<FIdUseExp>> res, CValue val) {
        Set<FIdUseExp> s = new HashSet<FIdUseExp>();
        s.addAll(val.getDependencies());
        s.add(this);
