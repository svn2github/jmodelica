optimization TestVariableTypes(objectiveIntegrand = x^2 + u_cont^2 + u_bf^2) 
    Real x(start=1, fixed=true);
    Real x2(start=0, fixed=true);
    Real w;
    Real w2 = w+10;
    input Real u_cont;
    input Real u_bf;
    parameter Real p(free=true, initialGuess=0);
equation
    der(x) = u_cont + w;
    w = -x + u_bf + p;
    der(x2) = x;
end TestVariableTypes;

optimization TestResiduals(startTime=0, finalTime=1)
    Real x;
    Real w;
    input Real u;
initial equation
    x = 1;
equation
    der(x) = u; 
constraint
    w = x^2;
    x <= u^2;
    x(finalTime) = 2;
    x(startTime) >= -5;
end TestResiduals;
