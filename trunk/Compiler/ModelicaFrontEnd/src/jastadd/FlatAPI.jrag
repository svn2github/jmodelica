		FAttribute res = findAttributeLocal(name);
		if (res == null && myFDerivedType() != null)
			res = myFDerivedType().findAttribute(name);
		return res;
	}
	syn FAttribute FAttribute.findAttribute(String name)        = findMatching(getFAttributes(), name);
