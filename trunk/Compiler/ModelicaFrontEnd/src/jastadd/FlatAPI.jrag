		if (root().options.getBooleanOption("equation_sorting")) {
			
			str.append("Variable dependencies:\n");
			
			str.append("Derivative variables: \n");
			for (FVariable fv : derivativeVariables()) {
				str.append(" " + fv.name() + "\n");
