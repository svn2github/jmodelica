aspect ReferencedVariables {
	public Set<FIdUseExp> ASTNode.findFIdUseExpsInTree() {
		Set<FIdUseExp> set = new LinkedHashSet<FIdUseExp>();
		findFIdUseExpsInTree(set);
		return set;
	}
	
	public void ASTNode.findFIdUseExpsInTree(Set<FIdUseExp> set) {
		for (ASTNode n : this)
			n.findFIdUseExpsInTree(set);
	}
	
	public void FIdUseExp.findFIdUseExpsInTree(Set<FIdUseExp> set) {
		set.add(this);
		super.findFIdUseExpsInTree(set);
	}
	
    public void FAttribute.findFIdUseExpsInTree(Set<FIdUseExp> set) {
    }
	
	syn Set<FIdUseExp> FAbstractEquation.FIdUseExpsInRHS() = Collections.emptySet();
	eq FEquation.FIdUseExpsInRHS()                         = getRight().findFIdUseExpsInTree();
	eq FFunctionCallEquation.FIdUseExpsInRHS()             = getCall().findFIdUseExpsInTree();
	eq FAlgorithm.FIdUseExpsInRHS() {
		Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
		for (FStatement stmt : getFStatements())
			res.addAll(stmt.FIdUseExpsInRHS());
		return res;
	}
	syn Set<FIdUseExp> FStatement.FIdUseExpsInRHS() {
		Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
		for (FIdUseExp use : findFIdUseExpsInTree())
			if (!use.useIsAssignment() && !use.myFV().isForIndex())
				res.add(use);
		res.removeAll(FIdUseExpsInLHS());
		return res;
	}
	
    syn Set<FIdUseExp> FIfWhenElseEquation.FIdUseExpsInRHS() {
        Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
        for (FAbstractEquation fae : getFAbstractEquations())
            res.addAll(fae.FIdUseExpsInRHS());
        return res;
    }
    
    syn Set<FIdUseExp> FIfWhenEquation.FIdUseExpsInRHS() {
        Set<FIdUseExp> res = super.FIdUseExpsInRHS();
        getTest().findFIdUseExpsInTree(res);
        if (hasElse())
            res.addAll(getElse().FIdUseExpsInRHS());
        return res;
    }
    
	syn Set<FIdUseExp> FAbstractEquation.FIdUseExpsInLHS() = Collections.emptySet();
	eq FEquation.FIdUseExpsInLHS()                         = getLeft().findFIdUseExpsInTree();
	eq FFunctionCallEquation.FIdUseExpsInLHS()             = getLefts().findFIdUseExpsInTree();
	eq FAlgorithm.FIdUseExpsInLHS() {
		Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
		for (FStatement stmt : getFStatements())
			res.addAll(stmt.FIdUseExpsInLHS());
		return res;
	}
	
	syn Set<FIdUseExp> FStatement.FIdUseExpsInLHS() {
		Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
		for (FIdUseExp use : findFIdUseExpsInTree())
			if (use.useIsAssignment() && !use.myFV().isForIndex())
				res.add(use);
		return res;
	}
	
    syn Set<FIdUseExp> FIfWhenElseEquation.FIdUseExpsInLHS() {
        Set<FIdUseExp> res = new LinkedHashSet<FIdUseExp>();
        for (FAbstractEquation fae : getFAbstractEquations())
            res.addAll(fae.FIdUseExpsInLHS());
        return res;
    }
    
    syn Set<FIdUseExp> FIfWhenEquation.FIdUseExpsInLHS() {
        Set<FIdUseExp> res = super.FIdUseExpsInLHS();
        if (hasElse())
            res.addAll(getElse().FIdUseExpsInLHS());
        return res;
    }
    
	syn ArrayList<FIdUseExp> FAbstractEquation.uniqueFIdUseExpsInLHS() {
		ArrayList<FIdUseExp>   res   = new ArrayList<FIdUseExp>();
		Set<FAbstractVariable> visit = new LinkedHashSet<FAbstractVariable>();
		for (FIdUseExp e : FIdUseExpsInLHS()) {
			FAbstractVariable fv = e.myFV();
			if (!visit.contains(fv)) {
				visit.add(fv);
				res.add(e);
			}
		}
		return res;
	}
	
	public static Set<FVariable> ASTNode.lookupFVariablesInSet(Set<FIdUseExp> set) {
		Set<FVariable> res = new LinkedHashSet<FVariable>();
		for (FIdUseExp use : set)
			if (use.myFV() instanceof FVariable)
				res.add((FVariable) use.myFV());
		return res;
	}
	
	syn Set<FVariable> FAbstractEquation.referencedFVariablesInRHS() = lookupFVariablesInSet(FIdUseExpsInRHS());
	syn Set<FVariable> FAbstractEquation.referencedFVariablesInLHS() = lookupFVariablesInSet(FIdUseExpsInLHS());
	syn Set<FVariable> FExp.referencedFVariablesInFExp()             = lookupFVariablesInSet(findFIdUseExpsInTree());
	syn Set<FVariable> FVariable.referencedFVariablesInBindingExp()  = lookupFVariablesInSet(getBindingExp().findFIdUseExpsInTree());
	syn lazy Set<FVariable> FAbstractEquation.referencedFVariables() {
		Set<FVariable> res = new HashSet<FVariable>();
		res.addAll(referencedFVariablesInLHS());
		res.addAll(referencedFVariablesInRHS());
		return res;
	}
	
	public static Set<FVariable> ASTNode.parametersInSet(Set<? extends FVariable> set) {
		Set<FVariable> res = new LinkedHashSet<FVariable>();
		for (FVariable fv : set)
			if (fv.isParameter())
				res.add(fv);
		return res;
	}
	
	syn Set<FVariable> FFunctionCallEquation.referencedParametersInLHS() = parametersInSet(referencedFVariablesInLHS());
	syn Set<FVariable> FExp.referencedParametersInFExp()                 = parametersInSet(referencedFVariablesInFExp());

	public Set<Var> BiPGraph.lookupVarsInSet(Set<FIdUseExp> set) {
		Set<Var> res = new LinkedHashSet<Var>();
		for (FIdUseExp use : set) {
			Var var = getVariable(use.name());
			if (var != null)
				res.add(var);
		}
		return res;
	}
}

