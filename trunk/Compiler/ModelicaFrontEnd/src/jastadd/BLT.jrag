		public UnsolvedAlgorithmBlock(FAlgorithm alg, Collection<FVariable> activeVariables, Collection<Integer> assignedIndices) {
			super(alg, activeVariables, assignedIndices);
		}

        @Override
        public boolean isSolvable() {
            return false;
        }

	}
	
