     }
     
     public String FClass.nextTempVarName() {
    	 FlatVariableMap vars = variablesMap();
         String name;
         do {
             name = "temp_" + nextTempVarIndex++;
         } while (vars.lookup(name) != null);
         return name;
     }
     public String FFunctionDecl.nextTempVarName() {
    	 FlatVariableMap vars = variablesMap();
         String name;
         do {
