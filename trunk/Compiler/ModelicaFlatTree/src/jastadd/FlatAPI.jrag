    eq FDerivedType.getChild().isInDerivedType()      = true;
    eq FAbstractEquation.getChild().isInDerivedType() = false;
    eq FFunctionDecl.getChild().isInDerivedType()     = false;
    eq FClass.getChild().isInDerivedType()            = false;

